{
  "name": "crud-openapi",
  "type": "module",
  "version": "0.0.73",
  "scripts": {
    "css": "tailwindcss -o css.css",
    "tsc": "tsc",
    "up": "npm version patch -f",
    "pub": "npm run up && npm publish && git add . && git commit -m 'improvements' && git push",
    "types": "npx openapi-fetch-typescript ./public/openapi.json ./app/openapi-types.ts OpenApiTypes",
    "watch-types": "nodemon --watch public/openapi.json --exec \"npm run types\"",
    "init-db": "bun run src/makeRootDb.cli.ts",
    "start": "concurrently \"vercel dev\" \"npm:watch-types\""
  },
  "dependencies": {
    "@upstash/redis": "^1.32.0",
    "@upstash/vector": "^1.1.2",
    "actionschema": "^0.0.38",
    "ajv": "^8.12.0",
    "from-anywhere": "^0.0.29",
    "json-schema": "^0.4.0",
    "openai": "^4.52.7",
    "openapi-for-humans-react": "^0.0.46",
    "openapi-types": "^12.1.3",
    "openapi-typescript": "^6.7.5",
    "openapi-util": "^0.0.48",
    "qs": "^6.12.1"
  },
  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20.14.10",
    "@types/qs": "^6.9.15",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "nodemon": "^3.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "license": "ISC",
  "description": "",
  "bin": {}
}
